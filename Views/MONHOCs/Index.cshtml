@model IEnumerable<WebQuanLyLopHoc_Nhom8.Models.MONHOC>

@{
    ViewBag.Title = "Index";
}

<h2>Danh sách môn học</h2>

<p>
    @Html.ActionLink("Thêm môn học mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Tên môn học
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TenMH)
            </td>
            <td>
                @Html.ActionLink("Xóa", "Delete", new { id = item.MaMH })
            </td>
        </tr>
    }

</table>
